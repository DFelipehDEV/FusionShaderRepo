<effect>
<name>Copper Bar FX</name>
<description>Amiga Copper Bar dx9</description>
<author>Gigatron</author>
<parameter>
	<name>Timer</name>
	<description>Timer Value ; increase this value always set effect parameter 'tt' to  Timer*0.002 to reverse direction Timer*-0.002</description>
	<code>tt</code>
	<type>float</type>
	<value>0.005</value>
</parameter>
<parameter>
	<name>Colors</name>
	<description>Colors of Bars</description>
	<code>clr</code>
	<type>int_float4</type>
             <property>Color</property>
	<value>65535</value>
</parameter>
<parameter>
    <name>Resolution</name>
    <description>Resolution of Bars</description>
    <code>rez</code>
    <type>float</type>
    <value>128.0</value>
</parameter>
<parameter>
    <name>Frequency</name>
    <description>Frequency of Bars</description>
    <code>freq</code>
    <type>float</type>
    <value>30.0</value>
</parameter>
 </effect>

