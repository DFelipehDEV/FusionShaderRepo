<effect>
<name>8-bit Sprite</name>
<author>MuddyMole / Sketchy</author>
<description>256 colour palette, flip/mirror, alpha dithering.</description>
<parameter>
	<name>Palette Image (16 x 16 Pixels).</name> <code>Palette</code>
	<type>IMAGE</type>
	<property>IMAGE</property>
	<value>8BitSprite_Example.png</value>
</parameter>
<parameter>
		<name>iFlipX</name>
		<code>iFlipX</code>
		<description>Flip horizontally.</description>
		<type>int</type>
		<property>checkbox</property>
		<value>0</value>
		<preview_value>0</preview_value>
</parameter>
<parameter>
		<name>iFlipY</name>
		<code>iFlipY</code>
		<description>Flip vertically.</description>
		<type>int</type>
		<property>checkbox</property>
		<value>0</value>
		<preview_value>0</preview_value>
</parameter>
<parameter>
		<name>iRotate</name>
		<code>iRotate</code>
		<description>Rotate 90 degrees (use only on square images).</description>
		<type>int</type>
		<property>checkbox</property>
		<value>0</value>
		<preview_value>0</preview_value>
</parameter>
<parameter>
		<name>fOpacity</name>
		<code>fOpacity</code>
		<description>Opacity of object.</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<preview_value>1.0</preview_value>
</parameter>
</effect>