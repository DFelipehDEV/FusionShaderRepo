<effect>
<name>Cut through and parallax</name>
<description>Cuts any background shape with non-black color through source image showing "texture" background, background texture can be parallaxed by setting offset and coefficient values</description>
<author>schrodinger</author>
<company>Lizard King</company>
<copyright>Free for commercial and not commercial, no attribution needed</copyright>
<BackgroundTexture>1</BackgroundTexture>
<parameter>
		<name>texture</name>
		<code>tximg</code>
		<description>background texture</description>
		<type>image</type>
		<property>image</property>
</parameter>
<parameter>
		<name>offset</name>
		<code>ox</code>
		<description>scroll offset</description>
		<type>float</type>
		<property>edit</property>
                                <value>0.0</value>
</parameter>
<parameter>
		<name>offset</name>
		<code>oy</code>
		<description>scroll offset</description>
		<type>float</type>
		<property>edit</property>
                                <value>0.0</value>
</parameter>
<parameter>
		<name>x_coeff</name>
		<code>xcoeff</code>
		<description>scroll offset</description>
		<type>float</type>
		<property>edit</property>
                                <value>0.0</value>
</parameter>
<parameter>
		<name>y_coeff</name>
		<code>ycoeff</code>
		<description>scroll offset</description>
		<type>float</type>
		<property>edit</property>
                                <value>0.0</value>
</parameter>
</effect>