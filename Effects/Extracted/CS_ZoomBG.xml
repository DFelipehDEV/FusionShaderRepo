<effect>
<name>Zoom Background</name>
<description>Zoom in and out</description>
<author>Sphax - Flavien Clermont</author>
<company>Complex-softwares, edit by NaitorStudios</company>
<website>http://complex.softwares.free.fr</website>
<parameter>
		<name>Width</name>
		<code>fWidth</code>
		<description>Width coefficient</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Height</name>
		<code>fHeight</code>
		<description>Height coefficient</description>
		<type>float</type>
		<property>edit</property>
		<value>1.0</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Zoom X</name>
		<code>fZoomX</code>
		<description>X center of the zoom</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5</value>
		<min>0.0</min>
</parameter>
<parameter>
		<name>Zoom Y</name>
		<code>fZoomY</code>
		<description>Y center of the zoom</description>
		<type>float</type>
		<property>edit</property>
		<value>0.5</value>
		<min>0.0</min>
</parameter>
<BackgroundTexture>1</BackgroundTexture>
<sampler>
<index>1</index>
<addressU>border</addressU>
<addressV>border</addressV>
